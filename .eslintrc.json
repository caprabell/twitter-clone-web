{
  "extends": "airbnb",
  "plugins": [
    "react",
    "import",
    "jsx-a11y"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "no-unused-expressions": 1,
    "react/no-unused-state": 0,
    "no-alert": 0,
    "react/destructuring-assignment": 0,
    "react/button-has-type": 0
  },
  "globals": {
    "document": true,
    "alert": true
  }
}
